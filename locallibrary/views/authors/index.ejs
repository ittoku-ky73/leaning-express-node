<h1><%= title %></h1>
<ul>
  <% if (authors) { %>
    <% authors.forEach((author)=> { %>
      <li>
        <a href=<%=author.url %>>
          <%= `${author.name} (${author.birth_date} - ${author.death_date})` %>
        </a>
      </li>
    <% }) %>
  <% } else { %>
    <li>There are no authors</li>
  <% } %>
</ul>
